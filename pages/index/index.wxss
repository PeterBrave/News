.title {
  display: flex;
  width: 100%;
  height: 130rpx;
  text-align: center;
  background-color: #65A1E2;
  z-index: -1;
}
.title-image {
  margin-top: 60rpx; 
  margin-left: 30rpx;
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
  background-color: #fff;
}
.title-text {
  margin-top: 60rpx; 
  margin-left: 15rpx;
  font-size: 32rpx;
  line-height: 45rpx;
  color: #fff;
}